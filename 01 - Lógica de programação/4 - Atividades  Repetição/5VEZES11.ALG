Algoritmo "5vezes11"

Var
   somaTotal: inteiro
   numeroAtual: inteiro

Inicio
   enquanto (numeroAtual <= 10) faca
            escreval (numeroAtual)
            somaTotal <- somaTotal + numeroAtual
            numeroAtual <- numeroAtual + 1
   fimEnquanto

   escreval("A soma total desses números é ", somaTotal, "!")

Fimalgoritmo